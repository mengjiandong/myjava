public class PropertiesUtils {
	
	Logger logger = LoggerFactory.getLogger(PropertiesUtils.class);
	
	public void testProperties() {
		Properties p = new Properties();
		try {
			p.load("configs/server.properties");
		} catch(IOException e) {
			
		}
	}

}
